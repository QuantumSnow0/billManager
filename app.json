{
  "expo": {
    "name": "BillManager",
    "slug": "billmanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/blogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/blogo.png",
        "backgroundColor": "#002676"
      },

      "package": "com.bonface254.billmanager"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/blogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/blogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#002676"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ffd9e9dc-d502-45ca-b281-3497abd43b12"
      }
    },
    "owner": "bonface254"
  }
}
